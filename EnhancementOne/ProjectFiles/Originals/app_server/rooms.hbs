<!DOCTYPE html>
<!-- Website template by freewebsitetemplates.com -->
<html>
<head>
	<meta charset="UTF-8">
	<title> Travlr Getaways - Rooms</title>
	<link rel="stylesheet" href="css/style.css" type="text/css">
</head>
<body>
	<div id="background">
		<div id="page">
			{{> header}}
			<div id="contents">
				<div class="box">
					<div>
						<div class="body">
							<h1>Rooms</h1>
							<ul id="rooms">
                                {{#each roomDeals}}
								<li>
									<a href="/roomDeals"><img src="images/{{this.image}}" alt="Img"></a>
									<h2><a href="/roomDeals">{{this.name}}</a></h2>
									{{{this.description}}}
									<span {{this.rate}}</span>
								</li>
								{{/each}}
							</ul>
						</div>
					</div>
				</div>
			</div>
		</div>
		{{> footer}}
	</div>
</body>
</html>